config 'global'
	option	anon_swap       '0'
	option	anon_mount      '0'
	option	auto_swap       '1'
	option	auto_mount      '1'
	option	delay_root      '5'
	option  check_fs        '1'

config 'mount'
	option  target  	'/boot'
	option	device		'/dev/by-name/boot'
	option	options		'ro,sync'
	option	enabled		'1'

config 'mount'
	option  target  	'/boot-res'
	option	device		'/dev/by-name/boot-res'
	option	options		'ro,sync'
	option	enabled		'1'

config 'mount'
	option  target  	'/mnt/UDISK'
	option	device		'/dev/by-name/UDISK'
	option	options		'rw,async'
	option	enabled		'1'

config 'mount'
	option  target  	'/overlay'
	option	device		'/dev/by-name/rootfs_data'
	option	options		'rw,sync'
	option	enabled		'1'

config 'mount'
	option  target  	'/mnt/SDCARD'
	option	device		'/dev/mmcblk1p1'
	option	options		'rw,async'
	option	enabled		'1'

config 'mount'
	option  target  	'/mnt/SDCARD'
	option	device		'/dev/mmcblk1p1'
	option	options		'rw,async'
	option	enabled		'1'

